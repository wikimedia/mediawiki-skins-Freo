<header class="skin-freo-siteheader mw-header">

	{{#data-logos}}
		<a href="{{link-mainpage}}" class="mw-logo">
			{{#icon}}
				<img class="mw-logo-icon" src="{{.}}" alt="" aria-hidden="true">
			{{/icon}}
			{{#wordmark}}
				<img class="mw-logo-wordmark" alt="{{msg-sitetitle}}" src="{{src}}" style="{{style}}">
			{{/wordmark}}
			{{^wordmark}}
				<strong>{{msg-sitetitle}}</strong>
			{{/wordmark}}
		</a>
	{{/data-logos}}

	<nav>
		{{{html-freo-menus}}}
	</nav>
	{{#data-search-box}}
		<div class="skin-freo-searchform-wrapper">
			<form action="{{form-action}}" id="skin-freo-searchform" class="skin-freo-searchform-nojs">
				<input type="hidden" name="title" value="{{page-title}}">
				{{{html-input}}}
				<button type="submit">{{msg-search}}</button>
			</form>
		</div>
	{{/data-search-box}}

	<div class="skin-freo-user-site-menus">
		<div class="skin-freo-menu">
			<button>{{msg-skin-freo-menu-user}}</button>
			<menu>
				{{{data-portlets.data-user-menu.html-items}}}
			</menu>
		</div>
		{{{html-freo-menu-site}}}
	</div>
</header>

<main>
	<header>
		{{#html-freo-associated-pages}}
			<ul class="skin-freo-associated-pages">{{{.}}}</ul>
		{{/html-freo-associated-pages}}
		<div class="skin-freo-title-menu">
			<h1 class="skin-freo-title firstHeading">{{!
				}}{{#page-title-url}}<a href="{{.}}">{{/page-title-url}}{{!
					}}{{#page-title-displaytitle}}{{{.}}}{{/page-title-displaytitle}}{{!
					}}{{^page-title-displaytitle}}{{page-title-text}}{{/page-title-displaytitle}}{{!
				}}{{#page-title-url}}</a>{{/page-title-url}}{{!
			}}</h1>

			<div class="skin-freo-menu skin-freo-menu-actions">
				<button>
					<span class="skin-freo-image-menu">
						<span>{{msg-skin-freo-menu-actions}}</span>
					</span>
				</button>
				<menu>
					{{{html-freo-page-actions}}}
				</menu>
			</div>
		</div>
		<div class="skin-freo-subtitle" id="mw-content-subtitle">{{{html-subtitleonly}}}</div>
	</header>

	<div id="content">
		{{{html-body-content}}}
	</div>
	{{{html-after-content}}}

	<footer>
	</footer>
</main>

<footer class="skin-freo-sitefooter">
	<div class="skin-freo-footer-blurb">{{{html-footer-blurb}}}</div>
</footer>
